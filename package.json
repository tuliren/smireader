{
  "name": "smireader",
  "version": "0.1.0",
  "description": "Read SMS (text) backups generated by old Siemens phones",
  "private": true,
  "scripts": {
    "parse": "python -m smireader.smireader",
    "lint": "npm run lint:black && npm run lint:isort && npm run lint:flake8",
    "lint:black": "python -m black .",
    "lint:isort": "python -m isort .",
    "lint:flake8": "python -m flake8 .",
    "format:write": "prettier --write \"**/*.{json,yaml,yml}\"",
    "format:check": "prettier --check \"**/*.{json,yaml,yml}\"",
    "test": "python -m pytest",
    "test:coverage": "python -m pytest --cov=smireader smireader/__tests__/",
    "install:dev": "pip install -e \".[dev]\""
  },
  "keywords": [
    "sms",
    "siemens",
    "backup"
  ],
  "author": "tuliren",
  "license": "MIT",
  "devDependencies": {
    "prettier": "^3.0.0"
  }
}
